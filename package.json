{
  "version": "1.0.1-0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "node": "./dist/inversify-esm.esm.js",
      "import": "./dist/inversify-esm.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "pretest": "npm run lint",
    "test": "dts test",
    "lint": "dts lint --fix",
    "prepare": "dts build",
    "prepublishOnly": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@abraham/reflection": "0.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint --fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "inversify-esm",
  "author": "Achmad Kurnianto",
  "module": "dist/inversify-esm.esm.js",
  "size-limit": [
    {
      "path": "dist/inversify-esm.cjs.production.min.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/inversify-esm.esm.js",
      "limit": "15 KB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/achmadk/inversify-esm.git"
  },
  "devDependencies": {
    "@abraham/reflection": "0.10.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@size-limit/webpack": "7.0.8",
    "@size-limit/webpack-why": "7.0.8",
    "@skypack/package-check": "0.2.2",
    "@types/jest": "28.1.0",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "babel-eslint": "10.1.0",
    "dts-cli": "1.5.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "^8.0.1",
    "jest": "28.1.0",
    "jest-watch-typeahead": "1.1.0",
    "prettier": "2.6.2",
    "size-limit": "^7.0.8",
    "ts-jest": "28.0.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "keywords": [
    "ioc",
    "di",
    "javascript",
    "typescript",
    "node",
    "dependency injection",
    "dependency inversion",
    "inversion of control container"
  ],
  "packageManager": "yarn@3.2.1",
  "stableVersion": "1.0.0"
}
