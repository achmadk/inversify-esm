{
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/inversify-esm.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "pretest": "npm run lint",
    "test": "tsdx test",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "prepublishOnly": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@abraham/reflection": "0.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "inversify-esm",
  "author": "Achmad Kurnianto",
  "module": "dist/inversify-esm.esm.js",
  "size-limit": [
    {
      "path": "dist/inversify-esm.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/inversify-esm.esm.js",
      "limit": "11 KB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/achmadk/inversify-esm.git"
  },
  "devDependencies": {
    "@abraham/reflection": "0.8.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@skypack/package-check": "0.2.2",
    "@types/jest": "27.0.1",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.9.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^7.0.2",
    "jest": "27.1.0",
    "prettier": "2.3.2",
    "size-limit": "^5.0.3",
    "ts-jest": "27.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "keywords": [
    "ioc",
    "di",
    "javascript",
    "typescript",
    "node",
    "dependency injection",
    "dependency inversion",
    "inversion of control container"
  ],
  "packageManager": "yarn@3.0.1"
}
